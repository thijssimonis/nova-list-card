{
    "name": "thijssimonis/nova-list-card",
    "description": "List rows in a table card.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "ThijsSimonis\\NovaListCard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ThijsSimonis\\NovaListCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix ."
      },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    }
}
